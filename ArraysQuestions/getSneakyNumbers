class Solution {
    public int[] getSneakyNumbers(int[] nums) {
        
        HashMap<Integer, Integer> map = new HashMap<>();
        for(int i: nums) map.put(i, map.getOrDefault(i, 0) + 1);

        int[] result = new int[2];

        int k =0;
        for(int i : map.keySet()) {
            if(map.get(i) == 2) result[k++] = i;
        }

        return result;
    }
}
