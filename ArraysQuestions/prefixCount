class Solution {
    public int prefixCount(String[] words, String pref) {
        
        int ans = 0;
        for(String s : words) {
            int count = 0;
            for(int i =0; i<pref.length(); i++) {
                if(pref.length() > s.length()) break;
                if(pref.charAt(i) == s.charAt(i)) {
                    count ++;
                }
            }

            if(count == pref.length()) ans++;
        }

        return ans;
    }
}



class Solution {
    public int prefixCount(String[] words, String pref) {
        int count = 0;

        for(String str: words){
            if(str.startsWith(pref)){
                count++;
            }
        }
        return count;
    }
}
