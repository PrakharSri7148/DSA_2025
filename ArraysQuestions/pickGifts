class Solution {
    public long pickGifts(int[] gifts, int k) {
 
        PriorityQueue<Integer> pq = new PriorityQueue(Collections.reverseOrder());
        for(int i : gifts) pq.offer(i);
        while( k != 0) {
            int a = pq.poll();
            pq.add((int)Math.sqrt(a));
            k--;
        }
        long sum = 0;
        while(!pq.isEmpty()) {
            sum += pq.poll();
        }
        return sum;
    }
}
