class Solution {
    public int specialArray(int[] nums) {
        
        int x = 0;
        int max = Integer.MIN_VALUE;
        for(int i : nums) {
            if(i > max) max = i;
        }

        while(x<=max) {
            int count = 0;
            for(int i=0; i<nums.length; i++) {
                if(nums[i] >= x) count++;
            }

            if(x == count) return x;
            x++;
        }

        return -1;

        
    }
}
