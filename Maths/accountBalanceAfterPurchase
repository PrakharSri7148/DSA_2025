class Solution {
    public int accountBalanceAfterPurchase(int purchaseAmount) {
        
        if(purchaseAmount % 10 == 0) return 100-purchaseAmount;

        int diff = purchaseAmount % 10;
        if( diff >= 5) {
            int diff2 = 10-diff;
            return 100 - (purchaseAmount + diff2);
        }

        int ans = purchaseAmount - diff;
        return 100 - ans;
    }
}
